    },
    {
      "cell_type": "code",
      "source": [
        "num = int(input(\"Enter your number: \"))\n",
        "if num > 0:\n",
        "  for i in range(2, int(num ** 0.5) + 1):\n",
        "    if num % i == 0:\n",
        "      print(\"No! its a prime number\")\n",
        "      break\n",
        "    else:\n",
        "      print(\"Yes! its not a prime number\")\n",
        "else:\n",
